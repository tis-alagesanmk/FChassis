<base:TabPanel
		xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
		d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FChassis.UI.Settings.Laser.TabPanel"
		xmlns:base="FChassis.UI.Settings"
		>
	<TabControl 
			Margin="5"
			SelectionChanged="TabControl_SelectionChanged">
		<TabItem Header="Laser Cutting"/>
		<TabItem Header="DCA Page"/>
		<TabItem Header="Ramp Cyles"/>
		<TabItem Header="Close" Background="Black" Foreground="White"/>
	</TabControl>	
</base:TabPanel>
