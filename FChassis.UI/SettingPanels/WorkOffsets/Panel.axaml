<base:Panel
	    xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
	    d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FChassis.UI.Settings.WorkOffsets.Panel"
	    xmlns:base="FChassis.UI.Settings"
        >
	<DockPanel LastChildFill="True">
		<DockPanel DockPanel.Dock="Right" Width="100" LastChildFill="True">
			<Button DockPanel.Dock="Bottom" 
					Margin="5"
					Height="32"
					Background="Black" Foreground="White"
					Click="CloseBtn_Click">Close</Button>
			<TextBlock DockPanel.Dock="Bottom" 
					   HorizontalAlignment="Center"
					   Height="32">Import Position</TextBlock>
		</DockPanel>
		<DockPanel Background="AliceBlue"
				   Margin="5">
		</DockPanel>
	</DockPanel>
</base:Panel>
