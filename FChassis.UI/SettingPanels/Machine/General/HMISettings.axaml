<base:Panel
	    xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
		d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FChassis.UI.Settings.Machine.General.HMISettings"
		xmlns:vm="using:FChassis.UI.SettingPanels.Machine.ViewModel"
		xmlns:base="FChassis.UI.Settings"
		x:DataType="vm:HMISettingsViewModel"
		xmlns:controls="using:FChassis.UI.Controls"
		Background="White">
	<UserControl.DataContext>
		<vm:HMISettingsViewModel/>
	</UserControl.DataContext>
	<ScrollViewer>
		<Grid ColumnDefinitions="20* 20* 20* 20* 20*"/>
	</ScrollViewer>
</base:Panel>
