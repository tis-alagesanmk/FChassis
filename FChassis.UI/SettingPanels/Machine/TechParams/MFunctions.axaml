<base:Panel xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:Class="FChassis.UI.Settings.Machine.TechParams.MFunctions"
             xmlns:base="FChassis.UI.Settings">
	<UserControl.Styles>
		<!--TextBlock Styles-->
		<Style Selector="TextBlock">
			<Setter Property="Margin" Value="5 0 0 0"/>
			<Setter Property="Foreground" Value="White"/>
			<Setter Property="HorizontalAlignment" Value="Left"/>
			<Setter Property="VerticalAlignment" Value="Center"/>
		</Style>

		<!--TextBox Styles-->
		<Style Selector="TextBox">
			<Setter Property="Margin" Value="5"/>
			<Setter Property="Padding" Value="1"/>
			<Setter Property="Width" Value="120"/>
			<Setter Property="Height" Value="25"/>
			<Setter Property="HorizontalAlignment" Value="Left"/>
		</Style>

		<!--Label Styles-->
		<Style Selector="Label">
			<Setter Property="VerticalAlignment" Value="Center"/>
			<Setter Property="HorizontalAlignment" Value="Right"/>
		</Style>
	</UserControl.Styles>
	<ScrollViewer>
		<Grid
		RowDefinitions="35,35,35,35,35,35,35,35,35,35,35
						35,35,35,35,35,35,35,35,35,35,35
						35,35,35,35,35,35,35,35,35,35,35"
		ColumnDefinitions="*,Auto,*">

			<Border Grid.Row="0" Grid.ColumnSpan="3" Background="Blue" Height="35">
				<TextBlock Text="Sub program table"/>
			</Border>

			<Label Grid.Row="1" Grid.Column="0" Content="M number 0"/>
			<TextBox Grid.Row="1" Grid.Column="1"/>

			<Label Grid.Row="2" Grid.Column="0" Content="M number 1"/>
			<TextBox Grid.Row="2" Grid.Column="1"/>

			<Label Grid.Row="3" Grid.Column="0" Content="M number 2"/>
			<TextBox Grid.Row="3" Grid.Column="1"/>

			<Label Grid.Row="4" Grid.Column="0" Content="M number 3"/>
			<TextBox Grid.Row="4" Grid.Column="1"/>

			<Label Grid.Row="5" Grid.Column="0" Content="M number 4"/>
			<TextBox Grid.Row="5" Grid.Column="1"/>

			<Label Grid.Row="6" Grid.Column="0" Content="M number 5"/>
