			<Label Grid.Row="13" Grid.Column="0" Content="M number 12"/>
			<TextBox Grid.Row="13" Grid.Column="1"/>

			<Label Grid.Row="14" Grid.Column="0" Content="M number 13"/>
			<TextBox Grid.Row="14" Grid.Column="1"/>

			<Label Grid.Row="15" Grid.Column="0" Content="M number 14"/>
